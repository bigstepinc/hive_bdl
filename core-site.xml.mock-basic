<configuration>
    <property>
        <name>fs.bdl.impl</name>
        <value>com.bigstep.bdl.BDLFileSystem</value>
    </property>

    <property>
        <name>fs.bdl.auth.username</name>
        <value>OBJ_STORAGE_USERNAME</value>
    </property>

    <property>
        <name>fs.bdl.auth.password</name>
        <value>OBJ_STORAGE_PASSWORD</value>
    </property>

    <!--MOCK ZONE-->
    <property>
        <name>bdl.conf.mock.audit.enabled</name>
        <value>AUDIT_ENABLE</value>
    </property>

    <!-- This way of setting the userId is highly unsafe and was put here only for testing purposes. -->
    <property>
        <name>bdl.conf.mock.audit.userId</name>
        <value>AUDIT_MOCK_USERID</value>
    </property>
    
    <property>
        <name>bdl.conf.mock.enabled</name>
        <value>true</value>
    </property>

    <property>
        <name>bdl.conf.mock.location</name>
        <value>SET_PATH_TO_CONF_FOLDER</value>
    </property>
    <!--/MOCK ZONE-->
</configuration>
